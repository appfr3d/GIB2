      "supportsTablet": true
    },
    "android": {
      "package": "com.gib.geomatikk",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB_waoXnAeBRlBly6grQNiE2hdlYvNv_gY"
        }
      }
    }
  }
